{
  "cli": {
    "version": ">= 16.26.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "artifactPath": "dist/smartpju-preview.apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "artifactPath": "dist/smartpju-v${version}.apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
